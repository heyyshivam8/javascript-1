//6.Write a program that takes a number and checks whether it is positive, negative, or zero.
function check(num) {
    if(num>0)
    {
        console.log("positive");
    }
    else if(num<0)
    {
        console.log("negative")
    }
    else{
        console.log("zero");
    }
}
console.log(check(5));
